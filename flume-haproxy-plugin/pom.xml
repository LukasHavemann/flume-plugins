    <artifactId>flume-haproxy-plugin</artifactId>
    <packaging>jar</packaging>
    <name>Flume HAProxy Plugin</name>
    <description>
        Event Sink Decorator which parse HAProxy log format in order to augment Flume Event attributes
    </description>
